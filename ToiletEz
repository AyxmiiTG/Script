}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Place"):InvokeServer(unpack(args))
wait(1)
        while true do
function getNil(name,class) for _,v in next, getnilinstances() do if v.ClassName==class and v.Name==name then return v;end end end

local args = {
    [1] = getNil("Leprechaun Cameraman", "Model")
}

game:GetService("ReplicatedStorage"):WaitForChild("Sell"):InvokeServer(unpack(args))

end
    else
        print("Dupe Off")
    end
end)
