local args = {
    [1] = "purchase",
    [2] = 10001,
    [3] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args))
