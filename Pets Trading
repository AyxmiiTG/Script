local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("Pets Trading Z", "Default")

local tab = DrRayLibrary.newTab("Dupe", "ImageIdHere")

tab.newToggle("Auto Dupe", "Ez", true, function(toggleState)
    if toggleState then
        while true do
local args = {
    [1] = {
        ["10130"] = 0.5
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Fuse"):WaitForChild("FuseRF"):InvokeServer(unpack(args))
local args = {
    [1] = {
        ["10040"] = 1
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Fuse"):WaitForChild("FuseRF"):InvokeServer(unpack(args))
local args = {
    [1] = {
        ["10080"] = 1
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Fuse"):WaitForChild("FuseRF"):InvokeServer(unpack(args))
local args = {
    [1] = {
        ["10096"] = 1
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Fuse"):WaitForChild("FuseRF"):InvokeServer(unpack(args))

    else
        print("Off")
    end
end)
