local Section = Tab:CreateSection("FAKE DROP FOR FREE!?!?!ðŸ˜±")
local Button = Tab:CreateButton({
   Name = "XRay Values(Prevent Fake Nothing Scammers Lol)",
   Callback = function()
game.Players.localPlayer.XRay.Value = true
   end,
})
local Button = Tab:CreateButton({
   Name = "Disable XRay(Button On Top Disables It Too)",
   Callback = function()
game.Players.localPlayer.XRay.Value = false
   end,
})
local Button = Tab:CreateButton({
   Name = "Fake Drop",
   Callback = function()
local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.RightHand:Destroy()
   end,
})
local Button = Tab:CreateButton({
   Name = "Reset Fake Drop",
   Callback = function()
local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.Head:Destroy()
   end,
})
local Button = Tab:CreateButton({
   Name = "Inf Jump",
   Callback = function()
local InfiniteJumpEnabled = true game:GetService("UserInputService").JumpRequest:connect(function() 	if InfiniteJumpEnabled then 		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
   end,
})
