local Button = Tab:CreateButton({
	Name = "Car godmode",
	Callback = function()
        _G.Car =true
        while _G.Car == true do
            wait(0.01)
            for i,v in pairs(game:GetService("Workspace").traffic:GetDescendants()) do
                if v:IsA("MeshPart") then
                    v.CanCollide = false
                    if v:FindFirstChild("TouchInterest") then
                        v.TouchInterest:Remove()
                    end
                end
            end
        end
	end,
})

