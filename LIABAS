_G.Trash = true 
_G.Sell = true
_G.Auto = true
_G.Are = true
_G.Change = true
_G.Sound = true



Rayfield:Notify("Hi 0:", "Welcome", 4483362458) -- Notfication -- Title, Content, Image

local Tab = Window:CreateTab("Farm", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Farm Functions")

local Toggle = Tab:CreateToggle({
	Name = "Auto Tool",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
        if state then
            _G.Auto = true
            while _G.Auto == true do
                game:GetService("RunService").RenderStepped:wait() 
                for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                    if v.ClassName == "Tool" then
                    v:Activate()
                end
            end
        end
    else
        _G.Auto = false
    end
	end,
})


local Toggle = Tab:CreateToggle({
	Name = "Auto-Sell-Tp",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
    if state then
        _G.Are = true
        local twen = 4
        while _G.Are == true do
            wait(0.01)
            local start = game.Players.LocalPlayer.Character.HumanoidRootPart
            local ens = game:GetService("Workspace").guy.Head

            local distance = (start.Position - ens.Position).Magnitude
            
            
            if math.floor(distance) >= 50 then
                twen = 4.5
            end

            if math.floor(distance) >= 85 then
                twen = 6.5
            end

            if math.floor(distance) >= 115 then
                twen = 7.5
            end
            if math.floor(distance) >= 150 then
                twen = 8.5
            end

            if math.floor(distance) >= 185 then
                twen = 9.5
            end


            if math.floor(distance) >= 200 then
                twen = 10.5
            end
            
            if game:GetService("Players").RodjerArc.PlayerGui.ScreenGui.dialogue.Text == "inventory full" then
                _G.Are = false
                print(twen)
                
                local TweenPart = game.Players.LocalPlayer.Character.HumanoidRootPart
                local TweenService = game:GetService("TweenService")

                local Info = TweenInfo.new(
                    twen, 
                    Enum.EasingStyle.Linear, 
                    Enum.EasingDirection.Out, 
                    0, 
                    true, 
                    0 
                )

                local TweenGoals = {
                    CFrame = CFrame.new(68.183136, 7.45033455, -85.3507233, 0.690459847, -3.82780172e-08, -0.723370731, -2.56127812e-08, 1, -7.73636728e-08, 0.723370731, 7.19440436e-08, 0.690459847)
                }

                local Tween = TweenService:Create(TweenPart,Info,TweenGoals)

                Tween:Play()
                wait(twen)
                _G.Are = true
            end
            

        end
    else
        _G.Are = false
    end
	end,
})

local Toggle = Tab:CreateToggle({
	Name = "Sell Aura",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
        if state then
            _G.Sell = true
            while _G.Sell == true do
                wait(0.1)
                sell()
            end
        else
            _G.Sell = false
        end
	end,
})

local Label = Tab:CreateLabel("Change-Only bus location :(")

local posbus = {
    Vector3.new(142.874, 4.55281, -272.02),
    Vector3.new(127.175, 9.49476, -245.267),
    Vector3.new(130.736, 8.05248, -224.652),
    Vector3.new(178.341, 6.588, -233.755),

}

local Slider = Tab:CreateSlider({
	Name = "Every/sec:",
	Range = {12, 120},
	Increment = 1,
	Suffix = "Sec:",
	CurrentValue = 12,
	Flag = "Slider1", 
	Callback = function(s)
        Sec = s
	end,
})

local Toggle = Tab:CreateToggle({
	Name = "Change Trash",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
        if state then
            _G.Change = true
            local i = 1

            while _G.Change == true do
            
                if i == 1 then
                    i -= 1
                    local rand = math.random(1,4)
                    game.Players.LocalPlayer.Character.Humanoid:MoveTo(posbus[rand])
                end
            
                wait(Sec)
                i += 1
            end
        else
            _G.Change = false
        end
	end,
})

local Label = Tab:CreateLabel("Robery")



local Toggle = Tab:CreateToggle({
	Name = "Auto-Grab-Beans",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
        if state then
            _G.Grab = true
            while _G.Grab == true do
                wait(0.1)
                for i,v in pairs(game:GetService("Workspace").Store.Robbery.Stealables:GetChildren()) do
                    if v.Name == "Beans" then
                        fireproximityprompt(v.ProximityPrompt, 1, true)
                    end
                end
            end
        else
            _G.Grab = false
        end
	end,
})

local Tab = Window:CreateTab("Visuals", 4483362458)

local Section = Tab:CreateSection("Visual Functions")


local Button = Tab:CreateButton({
	Name = "Trash Max Hitbox",
	Callback = function()
		for i,v in pairs(game:GetService("Workspace").hitboxes:GetChildren()) do
            v.Size = Vector3.new(20,6,20)
        end
	end,
})


local Toggle = Tab:CreateToggle({
	Name = "Visualiz Hitbox",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
        if state then
            for i,v in pairs(game:GetService("Workspace").hitboxes:GetChildren()) do
                v.Transparency = 0.5
                v.Color = Color3.new(0, 0, 165)
            end
        else
            for i,v in pairs(game:GetService("Workspace").hitboxes:GetChildren()) do
                v.Transparency = 1
                v.Color = Color3.new(0, 0, 165)
            end
        end
	end,
})



local Toggle = Tab:CreateToggle({
	Name = "Off-Notification",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(state)
        if state then
            game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.dialogue.TextTransparency = 1
            game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.dialogue.dialogueScript.Disabled = false
        else
                 game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.dialogue.TextTransparency = 0
            game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.dialogue.dialogueScript.Disabled = false
        end
	end,
})

